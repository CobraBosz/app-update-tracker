<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_close_extension_g_icon_no_theme" layout="@layout/softkey_icon_no_theme" content_description="@string/gboard_close_extension_and_back_to_letter_keyboard_desc">
            <action type="PRESS" keycode="CLOSE_EXTENSION" data="prime" />
            <icon location="@id/icon" value="@drawable/ic_google_search_trim" />
        </softkey>
        <softkey id="@id/softkey_close_extension_back_arrow_no_theme_m2" layout="@layout/softkey_icon_no_theme" content_description="@string/gboard_close_extension_and_back_to_letter_keyboard_desc">
            <action type="PRESS" keycode="CLOSE_EXTENSION" data="prime" />
            <icon location="@id/icon" value="@drawable/quantum_ic_search_grey600_24" />
        </softkey>
        <softkey id="@id/softkey_open_search_keyboard" layout="@layout/softkey_icon_no_theme" content_description="@string/gboard_gif_search_content_desc">
            <action type="PRESS" keycode="OPEN_EXTENSION_INTERNAL" data="com.google.android.apps.inputmethod.libs.search.INativeCardExtension" />
            <icon location="@id/icon" value="@drawable/ic_google_search_trim" />
        </softkey>
        <softkey id="@id/softkey_clear_and_open_search_no_theme" layout="@layout/softkey_icon_no_theme_with_padding" content_description="@string/gboard_cancel_query_content_desc">
            <action type="PRESS" keycode="OPEN_EXTENSION_AND_CLEAR_QUERY" data="com.google.android.apps.inputmethod.libs.search.INativeCardExtension" />
            <icon location="@id/icon" value="@drawable/quantum_ic_cancel_black_24" />
        </softkey>
    </softkeys>
</framework>