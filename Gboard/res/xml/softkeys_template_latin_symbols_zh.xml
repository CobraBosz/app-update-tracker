<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkey_template id="@+id/softkey_template_multi_popup_on_long_press_simple" layout="@layout/softkey_qwerty_label_character" popup_timing="@string/qwerty_popup_timing" slide_sensitivity="NORMAL" multi_touch="false" content_description="$desc$">
        <action type="PRESS" data="$press_data$" keycode="ALPHABET" intention="$press_intent=COMMIT$" popup_label="$press_data$" />
        <action type="LONG_PRESS" data="$long_press_data$" keycode="ALPHABET" intention="$long_press_intention=COMMIT$" merge_insertion_index="$merge_index=0$" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" />
        <label location="@id/label" value="$press_data$" />
    </softkey_template>
    <softkey_template id="@+id/softkey_template_fix_order_simple" layout="@layout/softkey_qwerty_label_character_label_header" popup_timing="@string/qwerty_popup_timing" slide_sensitivity="NORMAL" multi_touch="false" content_description="$desc$">
        <action type="PRESS" data="$press_data$" keycode="ALPHABET" intention="$press_intent=COMMIT$" popup_label="$press_data$" />
        <action type="LONG_PRESS" data="$long_press_data$" keycode="ALPHABET" intention="$long_press_intention=COMMIT$" merge_insertion_index="$merge_index=0$" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleCenteredArrayLayout" />
        <label location="@id/label" value="$press_data$" />
        <label location="@id/label_header" value="$hint_label$" />
    </softkey_template>
    <softkey_template id="@+id/softkey_template_no_popup_simple" layout="@layout/softkey_qwerty_label_character" popup_timing="@string/qwerty_popup_timing" slide_sensitivity="NORMAL" multi_touch="false" content_description="$desc$">
        <action type="PRESS" data="$press_data$" keycode="ALPHABET" intention="$press_intent=COMMIT$" popup_label="$press_data$" />
        <label location="@id/label" value="$press_data$" />
    </softkey_template>
</framework>