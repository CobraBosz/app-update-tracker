<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/click_background" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/browser_menu_divider_height" android:color="@color/browser_more_menu_divider_color" />
            <solid android:color="@color/solid_white" />
        </shape>
    </item>
</selector>